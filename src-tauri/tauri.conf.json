{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "RadRisks",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "save": true
      },
      "fs": {
        "writeFile": true
      },
      "shell": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "Â© 2023 EPA",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "gov.radrisks.epa",
      "longDescription": "A tool that allows a user to estimate the risk of different types of cancer from the inhalation or ingestion of radioactive material",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "resources/ingestion/*/*/*",
        "resources/inhalation/*/*/*"
      ],
      "shortDescription": "Radiation risk calculator",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "type": "embedBootstrapper"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "maximized": true,
        "resizable": true,
        "title": "RadRisks",
        "width": 1500
      }
    ]
  }
}